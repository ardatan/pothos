{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "1.4.9",
  "name": "@pothos-examples/complex-app",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "generate": "prisma generate && pnpm run schema",
    "schema": "node -r @swc-node/register scripts/build-schema.ts",
    "seed": "prisma migrate reset -f",
    "start": "node -r @swc-node/register src/server.ts",
    "type": "tsc --noEmit"
  },
  "prisma": {
    "seed": "node -r @swc-node/register prisma/seed.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@pothos/core": "^3.23.0",
    "@pothos/plugin-dataloader": "workspace:3.12.7",
    "@pothos/plugin-prisma": "workspace:3.35.8",
    "@pothos/plugin-prisma-utils": "workspace:0.4.7",
    "@pothos/plugin-relay": "workspace:3.28.7",
    "@pothos/plugin-scope-auth": "workspace:3.16.0",
    "@pothos/plugin-simple-objects": "workspace:3.6.7",
    "@pothos/plugin-validation": "workspace:3.8.7",
    "@prisma/client": "^4.6.1",
    "graphql": "16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-yoga": "3.0.3",
    "prisma": "^4.6.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
